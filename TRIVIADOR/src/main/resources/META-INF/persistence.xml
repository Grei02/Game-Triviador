<?xml version="1.0" encoding="UTF-8"?>
<persistence version="3.2" xmlns="https://java.ee/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_3_2.xsd">
  <persistence-unit name="Triviador" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>cr.ac.una.triviador.model.Achievements</class>
    <class>cr.ac.una.triviador.model.Answers</class>
    <class>cr.ac.una.triviador.model.Categories</class>
    <class>cr.ac.una.triviador.model.Game</class>
    <class>cr.ac.una.triviador.model.Players</class>
    <class>cr.ac.una.triviador.model.PlayersGame</class>
    <class>cr.ac.una.triviador.model.PlayersCategoriesGame</class>
    <class>cr.ac.una.triviador.model.PlayersWildcardGame</class>
    <class>cr.ac.una.triviador.model.QuestionsCategories</class>
    <class>cr.ac.una.triviador.model.Questions</class>
    <class>cr.ac.una.triviador.model.Wildcard</class>
    <properties>
      <property name="jakarta.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521/XEPDB1"/>
      <property name="jakarta.persistence.jdbc.user" value="tri"/>
      <property name="jakarta.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
      <property name="jakarta.persistence.jdbc.password" value="tri"/>
    </properties>
  </persistence-unit>
</persistence>
